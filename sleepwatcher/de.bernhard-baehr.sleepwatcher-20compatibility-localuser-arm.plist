<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple Computer//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>Label</key>
	<string>de.bernhard-baehr.sleepwatcher</string>
	<key>ProgramArguments</key>
	<array>
		<string>/opt/homebrew/sbin/sleepwatcher</string>
		<string>-V</string>
		<string>-s ~/.zsh/sleepwatcher/sleep-arm</string>
		<string>-w ~/.zsh/sleepwatcher/wakeup-arm</string>
	</array>
	<key>RunAtLoad</key>
	<true/>
	<key>KeepAlive</key>
	<true/>
	<!-- <key>StandardErrorPath</key>
  <string>/Users/reid/logs/sleepwatcher-arm.err</string>
	<key>StandardOutPath</key>
	<string>/Users/reid/logs/sleepwatcher-arm.out</string>  -->
</dict>
</plist>
