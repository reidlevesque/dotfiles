Include /Users/reid/.colima/ssh_config

Host github.com
  HostName github.com
  User git
  IdentityFile ~/.ssh/id_ed25519
  IdentitiesOnly yes

# To use ScaleFT proxycommand, add this configuration block to your $HOME/.ssh/config
# Match exec "/usr/local/bin/sft resolve -q  %h"
#     ProxyCommand "/usr/local/bin/sft" proxycommand --via asabastion-northamerica-northeast2  %h
#     UserKnownHostsFile "/Users/reid/Library/Application Support/ScaleFT/proxycommand_known_hosts"

Host rlevesque
  HostName rlevesque
  ForwardAgent yes
  ProxyCommand "/usr/local/bin/sft" proxycommand  %h
  IdentitiesOnly yes
  ControlMaster auto
  ControlPath ~/.ssh/ssh_mux_%h_%p_%r

Host *
  ServerAliveInterval 10
  ServerAliveCountMax 4
#  TCPKeepAlive yes
  ForwardAgent no
  StrictHostKeyChecking=accept-new
