Host enginfra-shared
  HostName enginfra-shared
  User rlevesque_nvidia_com
  IdentityFile ~/.ssh/google_compute_engine
  ProxyCommand /opt/homebrew/bin/gcloud compute start-iap-tunnel %h %p --listen-on-stdin --project=nv-hwlpu-20260103060253 --zone=us-central1-a

Host devvm-rlevesque
  HostName devvm-rlevesque
  User rlevesque_nvidia_com
  IdentityFile ~/.ssh/google_compute_engine
  ProxyCommand /opt/homebrew/bin/gcloud compute start-iap-tunnel %h %p --listen-on-stdin --project=nv-hwlpu-20260103060253 --zone=us-central1-a

Host *
  ServerAliveInterval 10
  ServerAliveCountMax 4
#  TCPKeepAlive yes
  ForwardAgent no
  StrictHostKeyChecking=accept-new
  # Use connection multiplexing to reuse existing connections
  ControlMaster auto
  ControlPath ~/.ssh/sockets/%r@%h-%p
  ControlPersist 600
